#%RAML 0.8
title: Cam's Test API
#baseUri: http://server/api/{version}
baseUri: http://mocksvc.mulesoft.com/mocks/ecad3134-5b5e-4e3d-8c5c-ec505a88eac6/api/{version}
version: v0.1
/books:
  displayName: Books
  get:
    description: Returns a list of all books
  post:
    description: Adds a book to the books collection
  /{bookid}:
    displayName: getBook
    get:
      description: Returns the specified book
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  name : "Moby Dick",
                  author : "The Author"
                }
    delete:
      description: Deletes the specified book